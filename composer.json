{
    "name": "php-etl/fast-map-config",
    "description": "This library implements the Extract-Transform-Load pattern asynchronously in PHP with the help of iterators and generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "symfony/expression-language": "^5.2",
        "symfony/property-access": "^5.2",
        "php-etl/metadata": "^0.2.0",
        "php-etl/mapping-contracts": "^0.2.0",
        "php-etl/fast-map": "dev-feature/update-satellite-toolbox"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\FastMapConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\FastMapConfig\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpunit/php-invoker": "*"
    }
}
